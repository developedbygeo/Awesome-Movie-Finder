{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2017",
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["components/*"],
      "@/pages/*": ["pages/*"],
      "@/hooks/*": ["hooks/*"],
      "@/shared/*": ["shared/*"],
      "@/styles/*": ["styles/*"],
      "@/api/*": ["pages/api/*"],
      "@/UI/*": ["components/UI/*"],
      "@/utils/*": ["utils/*"],
      "@/data/*": ["data/*"],
      "@/mock/*": ["mock/*"]
    }
  }
}
